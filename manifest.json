{
    "manifest_version": 2,
    "name": "Mul Mantar",
    "version": "1.0",
    "description": "A Chrome extension that shows the Sikh Mul Mantar on every new tab",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": [
      "storage"
    ],
    "icons": {
      "48": "icon48.png"
    }
  }